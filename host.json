{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "extensions": {
    "http": {
      "maxRequestBodySize": 209715200,
      "routePrefix": "api"
    },
    "blobs": {
      "maxDegreeOfParallelism": 4
    }
  },
  "functionTimeout": "00:05:00"
}
